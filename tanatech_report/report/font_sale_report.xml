<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="custom_sale_order_report" inherit_id="sale.report_saleorder_document">
            <xpath expr="//tbody" position="attributes">
                <attribute name="style">color: black!important;</attribute>
            </xpath>
            <xpath expr="//div[@t-field='doc.partner_id']" position="attributes">
                <attribute name="style">color: black!important;</attribute>
            </xpath>
            <xpath expr="//div[@class='page']" position="attributes">
                <attribute name="style">color: black!important;</attribute>
            </xpath>
            <xpath expr="//div[@class='clearfix']" position="attributes">
                <attribute name="style">color: black!important;</attribute>
            </xpath>
        </template>
    </data>
</odoo>
