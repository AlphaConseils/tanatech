<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>      
         <template id="custom_account_report_1" inherit_id="account.report_invoice_document">
            <xpath expr="." position="inside">
                <link rel="stylesheet" href="/tanatech_report/static/src/report.css"/>
            </xpath>
           <xpath expr="//tbody" position="attributes">
                <attribute name="style">color: black!important;</attribute>
            </xpath>
            <xpath expr="//span" position="attributes">
                <attribute name="style">color: black!important;</attribute>
            </xpath>
            <xpath expr="//address" position="attributes">
                <attribute name="style">color: black!important;</attribute>
            </xpath>
            <xpath expr="//p[@name='payment_communication']" position="attributes">
                <attribute name="style">color: black!important;</attribute>
            </xpath>
            <xpath expr="//td" position="attributes">
                <attribute name="style">color: black!important;</attribute>
            </xpath>
            <xpath expr="//b" position="attributes">
                <attribute name="style">color: black!important;</attribute>
            </xpath>
             <xpath expr="//strong" position="attributes">
                <attribute name="style">color: black!important;</attribute>
            </xpath>
             <xpath expr="//div" position="attributes">
                <attribute name="style">color: black!important;</attribute>
            </xpath>
            <xpath expr="//td" position="attributes">
                <attribute name="style">color: black!important;</attribute>
            </xpath>
            <xpath expr="//h2" position="attributes">
                <attribute name="style">color: black!important;</attribute>
            </xpath>
              <xpath expr="//div[@class='page']" position="attributes">
                <attribute name="style">color: black!important;</attribute>
            </xpath>
        </template> 
    </data>
</odoo>
